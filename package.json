{
  "name": "taro-demo",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "templateInfo": {
    "name": "taro-ui",
    "typescript": false,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:h5": "concurrently \" npm run build:h5 -- --watch\" \"node file-watch.js\"",
    "dev:rn": "concurrently \"node file-watch.js\" \" npm run build:rn -- --watch\"",
    "dev:qq": "concurrently \"node file-watch.js\" \" npm run build:qq -- --watch\"",
    "dev:jd": "concurrently \"node file-watch.js\" \" npm run build:jb -- --watch\"",
    "dev:alipay": "concurrently \"node file-watch.js\" \" npm run build:alipay -- --watch\"",
    "dev:tt": "concurrently \"node file-watch.js\" \" npm run build:tt -- --watch\"",
    "template": "node template-create.js"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/cli": "3.0.17",
    "@tarojs/components": "3.0.17",
    "@tarojs/react": "3.0.17",
    "@tarojs/redux": "^2.2.10",
    "@tarojs/redux-h5": "^2.2.10",
    "@tarojs/runtime": "3.0.17",
    "@tarojs/taro": "3.0.17",
    "chokidar": "^3.4.3",
    "concurrently": "^5.3.0",
    "dva-core": "^2.0.4",
    "dva-loading": "^3.0.21",
    "lodash": "4.17.15",
    "react": "^16.10.0",
    "react-dom": "^16.10.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "taro-ui": "^3.0.0-alpha.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "^3.0.18",
    "@tarojs/webpack-runner": "3.0.17",
    "@types/react": "^16.0.0",
    "@types/webpack-env": "^1.13.6",
    "babel-preset-taro": "3.0.17",
    "chalk": "^4.1.0",
    "ejs": "^3.1.5",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.0.17",
    "eslint-plugin-import": "^2.12.0",
